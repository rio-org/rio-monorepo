pragma solidity 0.8.21;

import {Test} from 'forge-std/Test.sol';
import {IVault} from 'contracts/interfaces/balancer/IVault.sol';
import {IManagedPoolFactory} from 'contracts/interfaces/balancer/IManagedPoolFactory.sol';
import {IWETH} from 'contracts/interfaces/misc/IWETH.sol';

bytes constant WETH_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant AUTHORIZER_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant VAULT_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant BALANCER_QUERIES_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant PROTOCOL_FEE_PROVIDER_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant MANAGED_POOL_AMM_LIB_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant EXTERNAL_WEIGHTED_MATH_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant RECOVERY_MODE_HELPER_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant MANAGED_POOL_FACTORY_CREATION_CODE = hex''; // forgefmt: disable-line

abstract contract BalancerDeployer is Test {
    address public constant ADMIN_ADDRESS = address(0xAD);

    address public constant WETH_ADDRESS = 0xC02aaA39b223FE8D0A0e5C4F27eAD9083C756Cc2;

    address public constant VAULT_ADDRESS = 0xBA12222222228d8Ba445958a75a0704d566BF2C8;
    address public constant BALANCER_QUERIES_ADDRESS = 0xE39B5e3B6D74016b2F6A9673D7d7493B6DF549d5;
    address public constant AUTHORIZER_ADDRESS = 0xA331D84eC860Bf466b4CdCcFb4aC09a1B43F3aE6;
    address public constant PROTOCOL_FEE_PROVIDER_ADDRESS = 0x97207B095e4D5C9a6e4cfbfcd2C3358E03B90c4A;
    address public constant MANAGED_POOL_AMM_LIB_ADDRESS = 0xb37646ACBb1A91287721B2CdBe01BA0DbA5E57A2;
    address public constant EXTERNAL_WEIGHTED_MATH_ADDRESS = 0x3db89f0CA3b388f2BcDbccD2ff8E13C22459CF75;
    address public constant RECOVERY_MODE_HELPER_ADDRESS = 0x7827108Af2fd9910EA7cf361d9b827941FA7D809;
    address public constant MANAGED_POOL_FACTORY_ADDRESS = 0xBF904F9F340745B4f0c4702c7B6Ab1e808eA6b93;

    IWETH public weth;
    IVault public vault;
    IManagedPoolFactory public managedPoolFactory;

    function deployBalancer() public {
        // Deploy WETH
        _deployTo(WETH_CREATION_CODE, new bytes(0), WETH_ADDRESS);

        // Deploy the Authorizer
        _deployTo(AUTHORIZER_CREATION_CODE, abi.encode(ADMIN_ADDRESS), AUTHORIZER_ADDRESS);

        // Deploy the Vault
        _deployTo(
            VAULT_CREATION_CODE,
            abi.encode(
                AUTHORIZER_ADDRESS,
                WETH_ADDRESS,
                7776000, // Pause Window Duration
                2592000 // Buffer Period Duration
            ),
            VAULT_ADDRESS
        );

        // Deploy the Balancer Queries Helper
        _deployTo(BALANCER_QUERIES_CREATION_CODE, abi.encode(VAULT_ADDRESS), BALANCER_QUERIES_ADDRESS);

        // Deploy the Protocol Fee Provider
        _deployTo(
            PROTOCOL_FEE_PROVIDER_CREATION_CODE,
            abi.encode(
                VAULT_ADDRESS,
                500000000000000000, // Max Yield Value
                500000000000000000 // Max AUM Value
            ),
            PROTOCOL_FEE_PROVIDER_ADDRESS
        );

        // Deploy Managed Pool AMM Lib
        _deployTo(MANAGED_POOL_AMM_LIB_CREATION_CODE, new bytes(0), MANAGED_POOL_AMM_LIB_ADDRESS);

        // Deploy External Weighted Math
        _deployTo(EXTERNAL_WEIGHTED_MATH_CREATION_CODE, new bytes(0), EXTERNAL_WEIGHTED_MATH_ADDRESS);

        // Deploy Recovery Mode Helper
        _deployTo(RECOVERY_MODE_HELPER_CREATION_CODE, abi.encode(VAULT_ADDRESS), RECOVERY_MODE_HELPER_ADDRESS);

        // Deploy the Managed Pool Factory
        _deployTo(
            MANAGED_POOL_FACTORY_CREATION_CODE,
            abi.encode(
                VAULT_ADDRESS,
                PROTOCOL_FEE_PROVIDER_ADDRESS,
                EXTERNAL_WEIGHTED_MATH_ADDRESS,
                RECOVERY_MODE_HELPER_ADDRESS,
                '{"name":"ManagedPoolFactory","version":2,"deployment":"20230411-managed-pool-v2"}', // Factory Version
                '{"name":"ManagedPool","version":2,"deployment":"20230411-managed-pool-v2"}', // Pool Version
                23328000, // Initial Pause Window Duration
                2592000 // Buffer Period Duration
            ),
            MANAGED_POOL_FACTORY_ADDRESS
        );

        weth = IWETH(WETH_ADDRESS);
        vault = IVault(VAULT_ADDRESS);
        managedPoolFactory = IManagedPoolFactory(MANAGED_POOL_FACTORY_ADDRESS);

        // Mint a bunch of WETH to the test account.
        weth.deposit{value: 1_000_000 ether}();
    }

    /// @dev Deploys a contract to the specified address.
    /// @param creationCode The contract creation code.
    /// @param args The contract constructor arguments.
    /// @param where The address to deploy the contract to.
    function _deployTo(bytes memory creationCode, bytes memory args, address where) internal virtual {
        vm.etch(where, abi.encodePacked(creationCode, args));
        (bool success, bytes memory runtimeBytecode) = where.call('');
        require(success, 'StdCheats deployCodeTo(string,bytes,uint256,address): Failed to create runtime bytecode.');
        vm.etch(where, runtimeBytecode);
    }
}
