pragma solidity 0.8.21;

import {Test} from 'forge-std/Test.sol';
import {MockERC20} from 'test/utils/MockERC20.sol';
import {IStrategy} from 'contracts/interfaces/eigenlayer/IStrategy.sol';
import {IStrategyManager} from 'contracts/interfaces/eigenlayer/IStrategyManager.sol';
import {UpgradeableBeacon} from '@openzeppelin/contracts/proxy/beacon/UpgradeableBeacon.sol';
import {TransparentUpgradeableProxy} from '@openzeppelin/contracts/proxy/transparent/TransparentUpgradeableProxy.sol';
import {IDelegationManager} from 'contracts/interfaces/eigenlayer/IDelegationManager.sol';
import {MockBeaconChainOracle} from 'test/utils/beacon-chain/MockBeaconChainOracle.sol';
import {MockBeaconChain} from 'test/utils/beacon-chain/MockBeaconChain.sol';

bytes constant EIGEN_POD_IMPL_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant EIGEN_POD_MANAGER_IMPL_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant STRATEGY_MANAGER_IMPL_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant DELEGATION_MANAGER_IMPL_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant BLS_PUBLIC_KEY_COMPENDIUM_CREATION_CODE = hex''; // forgefmt: disable-line
bytes constant STRATEGY_BASE_TVL_LIMITS_IMPL_CREATION_CODE = hex''; // forgefmt: disable-line

abstract contract EigenLayerDeployer is Test {
    address public constant OWNER_ADDRESS = address(0xAD);
    address public constant STRATEGY_WHITELISTER_ADDRESS = address(0xA1);

    address public constant ETH_POS_ADDRESS = 0xff50ed3d0ec03aC01D4C79aAd74928BFF48a7b2b;

    address public constant STRATEGY_MANAGER_ADDRESS = 0x779d1b5315df083e3F9E94cB495983500bA8E907;
    address public constant STRATEGY_MANAGER_IMPL_ADDRESS = 0x8676bb5f792ED407a237234Fe422aC6ed3540055;
    address public constant DELEGATION_MANAGER_ADDRESS = 0x1b7b8F6b258f95Cf9596EabB9aa18B62940Eb0a8;
    address public constant DELEGATION_MANAGER_IMPL_ADDRESS = 0x9b7980a32ceCe2Aa936DD2E43AF74af62581A99d;
    address public constant BLS_PUBLIC_KEY_COMPENDIUM_ADDRESS = 0xc81d3963087Fe09316cd1E032457989C7aC91b19;
    address public constant EIGEN_POD_IMPL_ADDRESS = 0x86bf376E0C0c9c6D332E13422f35Aca75C106CcA;
    address public constant EIGEN_POD_BEACON_ADDRESS = 0x3093F3B560352F896F0e9567019902C9Aff8C9a5;
    address public constant EIGEN_POD_MANAGER_ADDRESS = 0xa286b84C96aF280a49Fe1F40B9627C2A2827df41;
    address public constant EIGEN_POD_MANAGER_IMPL_ADDRESS = 0xdD09d95bD25299EDBF4f33d76F84dBc77b0B901b;
    address public constant DELAYED_WITHDRAWAL_ROUTER_ADDRESS = 0x89581561f1F98584F88b0d57c2180fb89225388f;
    address public constant SLASHER_ADDRESS = 0xD11d60b669Ecf7bE10329726043B3ac07B380C22;
    address public constant STRATEGY_BASE_TVL_LIMITS_IMPL_ADDRESS = 0x81E94e16949AC397d508B5C2557a272faD2F8ebA;

    address public constant RETH_STRATEGY = 0x879944A8cB437a5f8061361f82A6d4EED59070b5;
    address public constant CBETH_STRATEGY = 0x54945180dB7943c0ed0FEE7EdaB2Bd24620256bc;

    IStrategyManager public immutable strategyManager = IStrategyManager(STRATEGY_MANAGER_ADDRESS);
    IDelegationManager public immutable delegationManager = IDelegationManager(DELEGATION_MANAGER_ADDRESS);

    IStrategy public immutable rETHStrategy = IStrategy(RETH_STRATEGY);
    IStrategy public immutable cbETHStrategy = IStrategy(CBETH_STRATEGY);

    MockERC20 public rETH;
    MockERC20 public cbETH;

    address public RETH_ADDRESS;
    address public CBETH_ADDRESS;

    MockBeaconChain beaconChain;

    function deployEigenLayer() public {
        // Deploy LSTs
        rETH = new MockERC20('Rocket Pool ETH', 'rETH');
        cbETH = new MockERC20('Coinbase Wrapped Staked ETH', 'cbETH');

        RETH_ADDRESS = address(rETH);
        CBETH_ADDRESS = address(cbETH);

        // Deploy the mock beacon chain & beacon chain oracle
        MockBeaconChainOracle beaconChainOracle = new MockBeaconChainOracle();
        beaconChain = new MockBeaconChain(beaconChainOracle);

        // Deploy the Eigen Pod Implementation
        _deployTo(
            EIGEN_POD_IMPL_CREATION_CODE,
            abi.encode(
                ETH_POS_ADDRESS,
                DELAYED_WITHDRAWAL_ROUTER_ADDRESS,
                EIGEN_POD_MANAGER_ADDRESS,
                32000000000, // Max Restaked Balance Gwei Per Validator
                1616508000 // Genesis Time
            ),
            EIGEN_POD_IMPL_ADDRESS
        );

        // Deploy the EigenPod Upgradeable Beacon
        _deployTo(
            type(UpgradeableBeacon).creationCode,
            abi.encode(EIGEN_POD_IMPL_ADDRESS, OWNER_ADDRESS),
            EIGEN_POD_BEACON_ADDRESS
        );

        // Deploy the Eigen Pod Manager Implementation
        _deployTo(
            EIGEN_POD_MANAGER_IMPL_CREATION_CODE,
            abi.encode(
                ETH_POS_ADDRESS,
                EIGEN_POD_BEACON_ADDRESS,
                STRATEGY_MANAGER_ADDRESS,
                SLASHER_ADDRESS,
                DELEGATION_MANAGER_ADDRESS
            ),
            EIGEN_POD_MANAGER_IMPL_ADDRESS
        );

        // Deploy the EigenPod Manager
        _deployTo(
            type(TransparentUpgradeableProxy).creationCode,
            abi.encode(
                EIGEN_POD_MANAGER_IMPL_ADDRESS,
                OWNER_ADDRESS,
                abi.encodeWithSignature(
                    'initialize(uint256,address,address,address,uint256)',
                    type(uint256).max, // Max Pods
                    address(beaconChainOracle),
                    OWNER_ADDRESS,
                    address(1), // Pauser Registry
                    0 // Not Paused
                )
            ),
            EIGEN_POD_MANAGER_ADDRESS
        );

        // Deploy the Strategy Manager Implementation
        _deployTo(
            STRATEGY_MANAGER_IMPL_CREATION_CODE,
            abi.encode(DELEGATION_MANAGER_ADDRESS, EIGEN_POD_MANAGER_ADDRESS, SLASHER_ADDRESS),
            STRATEGY_MANAGER_IMPL_ADDRESS
        );

        // Deploy the Strategy Manager
        _deployTo(
            type(TransparentUpgradeableProxy).creationCode,
            abi.encode(
                STRATEGY_MANAGER_IMPL_ADDRESS,
                OWNER_ADDRESS,
                abi.encodeWithSignature(
                    'initialize(address,address,address,uint256)',
                    OWNER_ADDRESS,
                    STRATEGY_WHITELISTER_ADDRESS,
                    address(1),
                    0 // Not Paused
                )
            ),
            STRATEGY_MANAGER_ADDRESS
        );

        // Deploy the Delegation Manager Implementation
        _deployTo(
            DELEGATION_MANAGER_IMPL_CREATION_CODE,
            abi.encode(STRATEGY_MANAGER_ADDRESS, SLASHER_ADDRESS, EIGEN_POD_MANAGER_ADDRESS),
            DELEGATION_MANAGER_IMPL_ADDRESS
        );

        // Deploy the Delegation Manager
        _deployTo(
            type(TransparentUpgradeableProxy).creationCode,
            abi.encode(
                DELEGATION_MANAGER_IMPL_ADDRESS,
                OWNER_ADDRESS,
                abi.encodeWithSignature(
                    'initialize(address,address,uint256)',
                    OWNER_ADDRESS,
                    address(1),
                    0 // Not Paused
                )
            ),
            DELEGATION_MANAGER_ADDRESS
        );

        // Deploy the BLS Public Key Compendium
        _deployTo(BLS_PUBLIC_KEY_COMPENDIUM_CREATION_CODE, '', BLS_PUBLIC_KEY_COMPENDIUM_ADDRESS);

        // Deploy the Strategy Base TVL Limits Implementation
        _deployTo(
            STRATEGY_BASE_TVL_LIMITS_IMPL_CREATION_CODE,
            abi.encode(STRATEGY_MANAGER_ADDRESS),
            STRATEGY_BASE_TVL_LIMITS_IMPL_ADDRESS
        );

        // Deploy LST strategies
        _deployTo(
            type(TransparentUpgradeableProxy).creationCode,
            abi.encode(
                STRATEGY_BASE_TVL_LIMITS_IMPL_ADDRESS,
                OWNER_ADDRESS,
                abi.encodeWithSignature(
                    'initialize(uint256,uint256,address,address)',
                    type(uint256).max, // Max Per Deposit
                    type(uint256).max, // Max Total Deposits
                    RETH_ADDRESS, // Underlying Token
                    address(1) // Pauser Registry
                )
            ),
            RETH_STRATEGY
        );
        _deployTo(
            type(TransparentUpgradeableProxy).creationCode,
            abi.encode(
                STRATEGY_BASE_TVL_LIMITS_IMPL_ADDRESS,
                OWNER_ADDRESS,
                abi.encodeWithSignature(
                    'initialize(uint256,uint256,address,address)',
                    type(uint256).max, // Max Per Deposit
                    type(uint256).max, // Max Total Deposits
                    CBETH_ADDRESS, // Underlying Token
                    address(1) // Pauser Registry
                )
            ),
            CBETH_STRATEGY
        );

        // Whitelist strategies
        address[] memory strategies = new address[](2);
        strategies[0] = RETH_STRATEGY;
        strategies[1] = CBETH_STRATEGY;

        vm.prank(STRATEGY_WHITELISTER_ADDRESS);
        strategyManager.addStrategiesToDepositWhitelist(strategies);
    }

    /// @dev Deploys a contract to the specified address.
    /// @param creationCode The contract creation code.
    /// @param args The contract constructor arguments.
    /// @param where The address to deploy the contract to.
    function _deployTo(bytes memory creationCode, bytes memory args, address where) internal virtual {
        vm.etch(where, abi.encodePacked(creationCode, args));
        (bool success, bytes memory runtimeBytecode) = where.call('');
        require(success, 'StdCheats deployCodeTo(string,bytes,uint256,address): Failed to create runtime bytecode.');
        vm.etch(where, runtimeBytecode);
    }
}
